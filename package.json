{
  "name": "mockemon",
  "version": "4.2.1",
  "repository": "https://github.com/Glinkis/mockemon.git",
  "author": "Victor Glind√•s <glinkis@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "./builder": {
      "types": "./dist/builder.d.ts",
      "require": "./dist/builder.cjs",
      "default": "./dist/builder.js"
