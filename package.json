{
  "name": "mockemon",
  "version": "1.3.0",
  "repository": "https://github.com/Glinkis/mockemon.git",
  "author": "Victor Glind√•s <glinkis@gmail.com>",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./*": {
      "import": "./dist/esm/*",
      "require": "./dist/cjs/*",
      "types": "./dist/types/*"
    }
  },
  "scripts": {
    "test": "bun test",
    "build": "rm -rf ./dist/ && bun tsc -p tsconfig.esm.json && bun tsc -p tsconfig.cjs.json && bun tsc -p tsconfig.types.json",
    "prepublishOnly": "bun run build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@arktype/attest": "^0.1.1",
    "bun-types": "^1.0.11",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@faker-js/faker": "^8.2.0"
  },
  "peerDependenciesMeta": {
    "@faker-js/faker": {
      "optional": true
    }
  },
  "prettier": {
    "printWidth": 120
  },
  "keywords": [
    "mocking",
    "mock",
    "faker",
    "test",
    "testing",
    "builder",
    "factory"
  ]
}
