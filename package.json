{
  "name": "mockemon",
  "version": "2.0.1",
  "repository": "https://github.com/Glinkis/mockemon.git",
  "author": "Victor Glind√•s <glinkis@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "require": "./dist/builder.cjs"
      }
    },
    "./builder": {
      "import": {
        "types": "./dist/builder.d.ts",
        "import": "./dist/builder.js"
      },
      "require": {
        "types": "./dist/builder.d.cts",
        "require": "./dist/builder.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "bun test",
    "build": "bun run tsup",
    "prepublishOnly": "bun run build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "bun-types": "^1.0.11",
    "prettier": "^3.1.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "prettier": {
    "printWidth": 120
  },
  "keywords": [
    "mocking",
    "mock",
    "faker",
    "test",
    "testing",
    "builder",
    "factory"
  ]
}
