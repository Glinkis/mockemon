  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/bun": "^1.1.3",
    "@typescript-eslint/parser": "^7.11.0",
    "@typescript-eslint/rule-tester": "^7.11.0",
    "@typescript-eslint/utils": "^7.11.0",
    "expect-type": "^0.19.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "prettier": {
    "printWidth": 120
  },
  "keywords": [
    "mocking",
    "mock",
    "faker",
    "test",
    "testing",
    "builder",
    "factory"
