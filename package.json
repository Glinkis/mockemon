{
  "name": "mockemon",
  "version": "4.4.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Glinkis/mockemon.git"
  },
  "author": "Victor Glind√•s <glinkis@gmail.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./builder": {
      "import": {
        "types": "./dist/builder.d.mts",
        "default": "./dist/builder.mjs"
      },
      "require": {
        "types": "./dist/builder.d.ts",
        "default": "./dist/builder.js"
      }
    }
  },
  "scripts": {
    "test": "bun test",
    "build": "bun run tsup",
    "prepublishOnly": "bun run build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/bun": "^1.2.17",
    "expect-type": "^0.19.0",
    "prettier": "^3.6.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "prettier": {
    "printWidth": 120
  },
  "keywords": [
    "mocking",
    "mock",
    "faker",
    "test",
    "testing",
    "builder",
    "factory"
  ]
}
